<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.feevale.applogistica"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="br.feevale.applogistica.LoginActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!--
            <activity
            android:name="br.feevale.applogistica.LoginActivity"
            android:label="@string/title_activity_login" >
        </activity>
        -->
        <activity
            android:name="br.feevale.applogistica.LoginViewActivity"
            android:label="@string/login_view" >
        </activity>
        <activity
            android:name="br.feevale.applogistica.EntregasActivity"
            android:label="@string/title_activity_entregas" >
        </activity>
        <activity
            android:name="br.feevale.applogistica.ProdutosEntregaActivity"
            android:label="@string/title_activity_produtos_entrega" >
        </activity>
        <activity
            android:name="br.feevale.applogistica.DetalhesEntregaActivity"
            android:label="@string/title_activity_detalhes_entrega" >
